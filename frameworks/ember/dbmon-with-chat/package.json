{
  "name": "my-app",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "description": "Small description for my-app goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "vite build --mode test && ember test --path dist"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/runtime": "^7.27.6",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.2.0",
    "@embroider/core": "4.1.1",
    "@embroider/vite": "1.1.5",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "alpha",
    "@glint/environment-ember-loose": "alpha",
    "@glint/environment-ember-template-imports": "alpha",
    "@glint/template": "alpha",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^9.2.0",
    "decorator-transforms": "^2.3.0",
    "ember-eslint": "^0.5.0",
    "ember-modifier": "^4.2.2",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.5.0",
    "ember-template-lint": "^7.9.1",
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.0.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "imports": {
    "#app/*": "./app/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "dependencies": {
    "common": "link:../../../common",
    "tracked-built-ins": "^4.0.0"
  },
  "pnpm": {
    "overrides": {
      "@glimmer/component": "^2.0.0"
    }
  }
}
